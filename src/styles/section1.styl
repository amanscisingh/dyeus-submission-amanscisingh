@import 'variables'

.section1 
    position relative
    width 100%
    display flex
    flex-direction column 
    align-items center
    justify-content start
    background linear-gradient(180deg, #FFF6FB 0%, #E7F6FC 33.33%, rgba(213, 224, 255, 0) 100%)
    top 103px

.section1__element
    color blue
    font-weight 700
    width 80%

.section1__line
    border 1px solid #000000
    width 70%
    color #000
    background #000