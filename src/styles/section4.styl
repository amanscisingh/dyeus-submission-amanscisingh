@import 'variables'

.section4
    position relative
    background linear-gradient(180deg, #FFF6FB 0%, #FFFFFF 100%)
    text-align left
    width 100%
    display flex
    flex-direction column 
    align-items center
    justify-content start

.section4__element
    color blue
    width 80%

.section4__images
    display flex
    flex-direction row
    flex-wrap wrap
    width 100%
    align-content center
    justify-content center

.section4__images__img
    margin 20px
    width 190px
    height 190px
